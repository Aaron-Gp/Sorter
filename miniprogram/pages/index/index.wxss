.header{
  margin: 16rpx 20rpx;
  font-size: 32rpx;
  font-weight: 300;
}
.navigate{
  color: blue;
}
.viewWrapper{
  min-height: 800rpx;
}
.title{
  margin-left: 40rpx;
  font-size: 30rpx;
  font-weight: 350;
  margin-top: 20rpx;
}
.viewFirst, .viewSecond{
  margin-left: auto;
  margin-right: auto;
  width: 700rpx;
  min-height: 300rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  text-align: center;
}
.single{
  width: 90rpx;
  height: 90rpx;
  margin: 20rpx 10rpx;
  border: 2rpx dashed #eaff56;
}
image{
  width:100%;
  height:90rpx;
}
.single view{
  font-size: 16rpx;
}
button{
  top: 70rpx;
}
button[disabled][type='primary'] {
  color: rgba(0, 0, 0, 0.459) !important;
  background-color: #e9e7ef !important;
}